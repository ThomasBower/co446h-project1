# http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_style = space
indent_size = 4
tab_width = 8
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
max_line_length = 80

[*.html]
max_line_length = 120

[manifest.json]
indent_size = 4
